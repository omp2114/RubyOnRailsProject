<p id="notice"><%= notice %></p>
<p>
  <b>Professor:</b>
  <%= @course.name %>
</p>

<p>
  <b>Professor:</b>
  <%= @course.professor %>
</p>

<p>
  <b>Description:</b>
  <%= @course.description %>
</p>

<p>
  <b>Callnumber:</b>
  <%= @course.callnumber %>
</p>

<p>
  <b>Enrollment:</b>
  <%= @course.enrollment %>
</p>

<p>
  <b>Days:</b>
  <%= @course.days %>
</p>

<p>
  <b>Schedule:</b>
  <%= @course.schedule %>
</p>

<p>
  <b>Ta:</b>
  <%= @course.TA %>
</p>
<p>
  <b>Students:</b>
  <% @course.users.each do |user| %>
  <tr>
    <td><%= user.uni%></td>
  </tr>
  <% end %>
</p>


<p>
    <b> Announcements: </b>
  <%= @course.announcement %>
</p>

<% if admin? %>
  <%= link_to 'Edit', edit_course_path(@course) %> |
<%end%>
<%= link_to 'Back', courses_path %> 

